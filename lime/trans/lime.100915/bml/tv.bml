}
function GN_isfocus(){
	if(this.sub!=null && this.sub.length>0) return true;
	if(this.main!=null && this.main.length>0) return true;
	return false;
}
function GN_mainfocus(){
	playSound(9);
	setImg("bg_ctg03","images/bg_ctg03-f.png");
	this.showMainCsr();
	setFcs("bg_ctg03");
	setTicker("ctgmt2",2000,300,5,"　",this.mtitle,16,true);
