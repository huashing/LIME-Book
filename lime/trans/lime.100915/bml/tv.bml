function chlistkey(){
	var code = document.currentEvent.keyCode;
	var id = document.currentEvent.target.id;
	lockScreen();
	if(code==1){
		if(!gUpDownKey) {unlockScreen();return;}
		if(gState.downbox.idx>4&&gState.downbox.addIdx(gState.downbox.idx-5,true,true)){
			gState.downbox.movefocus();
		}else{
			if(gState.upbox.isfocus()){
				chlist_blur();
				gState.upbox.focus(code);
			}else{
				chlist_blur();
				scFocus();
			}
		}
	}else if(code==2){
		if(!gUpDownKey) {unlockScreen();return;}
		if(gState.downbox.addIdx(gState.downbox.idx+5,true,true)){
			gState.downbox.movefocus();
		}else{
			chlist_blur();
			scFocus();
		}
	}else if(code==3){
		if(!gSideKey) {unlockScreen();return;}
		if( ( (gState.downbox.idx) % 5) == 0 ){
			if(gState.downbox.maxPage>1){
				gState.downbox.idx=0;
				gState.downbox.nowPage--;
				if(gState.downbox.nowPage<0)gState.downbox.nowPage=gState.downbox.maxPage-1;
				romSound(9);
				if(gState.downbox.show(gState.ispid,gState.mode,gState.age)){
					gPlaySound=false;
					gState.downbox.movefocus();
				}
			}
		}else{
			if(gState.downbox.addIdx(gState.downbox.idx-1,true,true))gState.downbox.movefocus();
		}
	}else if(code==4){
		if(!gSideKey) {unlockScreen();return;}
		if( ( (gState.downbox.idx+1) % 5) == 0 || (gState.downbox.idx+1) >= gState.downbox.list.length){
			if(gState.downbox.maxPage>1){
				gState.downbox.idx=0;
				gState.downbox.nowPage++;
				if(gState.downbox.maxPage<=gState.downbox.nowPage)gState.downbox.nowPage=0;
				romSound(9);
				if(gState.downbox.show(gState.ispid,gState.mode,gState.age)){
					gPlaySound=false;
					gState.downbox.movefocus();
				}
			}
		}else{
			if(gState.downbox.addIdx(gState.downbox.idx+1,true,true))gState.downbox.movefocus();
		}
	}else if( (code >= 5) && (code <= 14) ){//数字キー
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		if(gState.downbox!=null){
			gState.downbox.numClick(code);
		}
	}else if(code==18){
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		romSound(7);
		var target=gState.downbox.get();
		if(target!=null && !interimCheck(target.period.displayStart,target.period.displayEnd)){
			chlist_blur();
			showPopup(new Array("閉じる"),lakeep,"エラー","\r\n提供期間外のためご利用いただけません。\r\n\r\n状態番号：19032","");
			unlockScreen();
			return;
		}
		if(gState.downbox.disable[gState.downbox.idx] == true){
			gPlaySound=false;
			showByContentInformation(target.crid,target.displayType,true);
		}else{
			var nId=parseInt(ORG_NETWORK_ID,10);
			var serId=parseInt(target.broadCastEvent.serviceId,10);
			if(!isNaN(nId)&&!isNaN(serId)){
				lockScreen();
				if(this.allHidden!=null)this.allHidden();
				showElem("loading2");
				unlockScreen();
				var ret=NaN;
				var ret=browser.launchDocument(gState.getUrl("iptv.bml") + "&"+padding("0000",nId.toString())+"&"+padding("0000",serId.toString(16)),"cut");
				if(isNaN(ret))launchHome();

			}
		}
	}else if(21<=code && code<=24){
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		if(gState.downbox!=null){
			if(gState.downbox.list==null||gState.downbox.list.length==0) {unlockScreen();return;}
			gState.downbox.colorClick(code);
		}
	}else if(code==19){
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		returnFunc();
	}
	unlockScreen();
}
