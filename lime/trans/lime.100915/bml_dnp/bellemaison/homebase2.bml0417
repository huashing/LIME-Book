<?xml version="1.0" encoding="EUC-JP" ?><!DOCTYPE bml PUBLIC "+//ARIB STD-B24:1999//DTD BML Document//JA" "http://www.arib.or.jp/B24/DTD/bml_1_1.dtd"><?bml bml-version="3.0" ?><bml>	<head>		<title/><script><![CDATA[var d=document;var b=browser;var Fid="tag3";var FM=new Array();	// U,D,L,R	FM["tag3"]=new Array("tag4","tag2","","");	FM["tag2"]=new Array("tag3","tag4","","");	FM["tag4"]=new Array("tag2","tag3","","");var pg=0;var KD=0;var K;function load(){	GE("bgm").streamStatus="play";	LK(1);	FC(Fid);	LK(0);	b.setInterval("BGMrp();",31000,0);}function FC(id){	VH(id,0);	VH(id+"d",0);	VH(id+"f",1);}function FB(id){	VH(id+"d",0);	VH(id+"f",0);	VH(id,1);}function DW(id){	VH(id,0);	VH(id+"f",0);	VH(id+"d",1);}function keyDown(){	if(KD==1) return;	K=d.currentEvent.keyCode;	// UDLR	if(1<=K && K<=4){		if(FM[Fid][(K-1)]=="") return;//		b.playRomSound("romsound://9");		LK(1);		FB(Fid);		Fid=FM[Fid][(K-1)];		LK(0);	}	if(K==18){		if(Fid!="tag3"){			GE("bgm").streamStatus="stop";			b.playRomSound("romsound://7");		}		LK(1);		DW(Fid);		LK(0);	}	if(K==19){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		FB(Fid);		LK(0);	}	KD=1;}function keyUp(){	if(KD==0) return;	// UDLR	if(1<=K && K<=4){		LK(1);		FC(Fid);		LK(0);	}	if(K==18){		if(Fid=="tag2") LC("homebase3.bml");		else if(Fid=="tag3"){			if(pg==0){				LK(1);				VH("background2",1);				VH("background1",0);				LK(0);				GE(Fid).data="normal_03_3.jpg";				GE(Fid+"f").data="focus_03_3.jpg";				LK(1);				FC(Fid);				LK(0);				GE(Fid+"d").data="keydown_03_3.jpg";				pg=1;			}			else{				LK(1);				VH("background1",1);				VH("background2",0);				LK(0);				GE(Fid).data="normal_03_2.jpg";				GE(Fid+"f").data="focus_03_2.jpg";				LK(1);				FC(Fid);				LK(0);				GE(Fid+"d").data="keydown_03_2.jpg";				pg=0;			}		}		else if(Fid=="tag4") LC("homebase1.bml");	}	if(K==19){		LC("homebase1.bml");	}	KD=0;}function GE(id){	return d.getElementById(id);}function VH(id,v){	GE(id).normalStyle.visibility=(v)?"visible":"hidden";}function LK(f){	if(f) b.lockScreen();	else b.unlockScreen();}function LC(bml){	GE("bgm").streamStatus="stop";	b.launchDocument(bml,"cut");}function BGMrp(){	GE("bgm").streamStatus="stop";	GE("bgm").streamStatus="play";}]]></script>	</head>	<body id="body1" onload="load();" style="resolution:960x540; display-aspect-ratio:16v9">		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="background1" style="left:0px; top:0px; width:960px; height:540px" data="background_2.jpg" type="image/jpeg"/>			<object id="background2" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_3.jpg" type="image/jpeg"/>		</div>		<div>			<object id="bgm" data="8s.aac" type="audio/X-arib-mpeg2-aac" streamstatus="stop"/>		</div>		<div id="btn" style="nav-index:0" onkeydown="keyDown();" onkeyup="keyUp();"/>		<div style="left:375px; top:360px; width:552px; height:99px; background-color-index:8">			<object id="tag2" style="left:0px; top:0px; width:552px; height:99px" data="normal_02_2.jpg" type="image/jpeg"/>			<object id="tag2f" style="left:0px; top:0px; width:552px; height:99px; visibility:hidden;" data="focus_02_2.jpg" type="image/jpeg"/>			<object id="tag2d" style="left:0px; top:0px; width:552px; height:99px; visibility:hidden;" data="keydown_02_2.jpg" type="image/jpeg"/>		</div>		<div style="left:852px; top:321px; width:177px; height:39px; background-color-index:8">			<object id="tag3" style="left:0px; top:0px; width:66px; height:27px" data="normal_03_2.jpg" type="image/jpeg"/>			<object id="tag3f" style="left:0px; top:0px; width:66px; height:27px; visibility:hidden;" data="focus_03_2.jpg" type="image/jpeg"/>			<object id="tag3d" style="left:0px; top:0px; width:66px; height:27px; visibility:hidden;" data="keydown_03_2.jpg" type="image/jpeg"/>		</div>		<div style="left:777px; top:480px; width:165px; height:45px; background-color-index:8">			<object id="tag4" style="left:0px; top:0px; width:165px; height:45px" data="normal_04_2.jpg" type="image/jpeg"/>			<object id="tag4f" style="left:0px; top:0px; width:165px; height:45px; visibility:hidden;" data="focus_04_2.jpg" type="image/jpeg"/>			<object id="tag4d" style="left:0px; top:0px; width:165px; height:45px; visibility:hidden;" data="keydown_04_2.jpg" type="image/jpeg"/>		</div>	</body></bml>