<script><![CDATA[
var PG=0;
var NX=0;
var PMX=20;
var NR="w";	// "f":パパ,"w":ママ,"c":じぶん
var WR="";	// "w":ワオ,"r":レオン
var PASS="";
var BTB=new Array();

var FID="";

var KNo;
var Key=0;
var CHG=1;

var RECORD=-1;
var TYPE=1;	// 0:フェードイン 1:カットイン
var Sound=0;
var Interval=10000;
var Gyou=0;

var PosCnt=0;

var AutPlay=0;

var PSI;
var FSI;
var TSI;
var MSI;

var Myself=0;
var W_BTB=new Array();
var R_BTB=new Array();
function load(){
	var i;
	if(UR(1)=="EHON"){
		PG=NUM( UR(2) );
		NR=UR(3);
		WR=UR(4);
		AutPlay=1;
	}
	UW(3,NR);
	// 絵本バイナリ取得
	BTB=GBT("ehon.bin","0,U:1B,U:1B,U:1B,U:1B,U:1B");
	if(BTB!=null){
		PMX=BTB.length;
		if(1<=PG){
			for(i=0; i<BTB.length; i++){
				if(PG==BTB[i][0]){
					RECORD=i;
					Interval=BTB[i][3];
					Gyou=BTB[i][4];
					AutPlay=1;
					break;
				}
			}
		}
	}
	W_BTB=GBT("ehon_w_txt.bin","0,U:1B,U:1B,U:1B,U:1B,U:1B,U:1B,U:1B,U:1B");
	R_BTB=GBT("ehon_r_txt.bin","0,U:1B,U:1B,U:1B,U:1B,U:1B,U:1B,U:1B,U:1B");
	PGSET();
	UW(1,"");
	CHG=0;
//	if(NR=="c") LoopSTR();
}
var LoopCnt=0;
function MLoop(){
	STT(0);
//LoopCnt++;
//DEBUG("ループ"+LoopCnt);
	STT(1);
}
function LoopSTR(){
	MSI=bro.setInterval("MLoop();",36000,0);
}
function LoopSTP(){
	if(!isNaN(MSI) && 0<=MSI) bro.clearTimer(MSI);
	MSI=-1;
}
function PGSET(){
//DEBUG(RECORD);
	if(Myself==1){
		LKS(1);
		WD("bl3",157);
		WD("rd3",159);
		WD("gr3",180);
		WD("yl3",170);
		WD("bl1",0);
		WD("rd1",0);
		WD("gr1",0);
		WD("yl1",0);
		LKS(0);
		LoopSTP();
		MLoop();
		LoopSTR();
	}
	else{
		LKS(1);
		if(0<=RECORD){
			WD("bl2",157);
			WD("rd2",159);
			WD("gr2",180);
			WD("yl2",170);
			WD("bl3",0);
			WD("rd3",0);
			WD("gr3",0);
			WD("yl3",0);
			WD("bl1",0);
			WD("rd1",0);
			WD("gr1",0);
			WD("yl1",0);
		}
		else{
			WD("bl1",157);
			WD("rd1",159);
			WD("gr1",180);
			WD("yl1",170);
			WD("bl3",0);
			WD("rd3",0);
			WD("gr3",0);
			WD("yl3",0);
			WD("bl2",0);
			WD("rd2",0);
			WD("gr2",0);
			WD("yl2",0);
			LoopSTP();
			MLoop();
			LoopSTR();
		}
		LKS(0);
		HON();
	}
//	if(GET("bgm").streamStatus=="stop") STT(1);
}
function HON(){
	var i,j,P,N,Pid,Pid2;
	if(0<=RECORD){
		if(NR!="c"){
			STT(0);
			// 音声切り替え
			if(0<=RECORD && NUM(BTB[RECORD][2]) ){
				LoopSTP();
//				DTC("bgm",NR+BTB[RECORD][0]+"_"+BTB[RECORD][2]+".aac");
				DTC("bgm",WR+NR+BTB[RECORD][0]+"_"+BTB[RECORD][2]+".aac");
//DEBUG3(WR+NR+BTB[RECORD][0]+"_"+BTB[RECORD][2]+".aac");
			}
			else{
				DTC("bgm","ehon.aac");
//DEBUG3("-");
			}
		}
TYPE=1;
		// フェードアウト
		if(TYPE==0){
			CHG=1;
			WD("p"+(NX-1),812);
			PosCnt=0;
			FD_OUT();
			FSI=bro.setInterval("FD_OUT();",200,0);
		}
		// カットアウト
		else{
			LKS(1);
			if(0<PG) Pid="p"+(PG-1);
			else Pid="p0";
			WD(Pid,0);
			LF(Pid+"d",0);
			if(BTB!=null){
				if(0<=RECORD){
					for(j=1; j<=Gyou; j++){
						WD(Pid+"_"+j,0);
					}
				}
				PG=BTB[RECORD][0];
				TYPE=BTB[RECORD][1];
				Interval=BTB[RECORD][3]*1000;
				if(NR=="c") Interval=30000;
				Gyou=BTB[RECORD][4];
			}
			Pid="p"+(PG-1);
			WD(Pid,812);
//DEBUG("PG="+PG+"; TYPE="+TYPE+"; Interval="+Interval+"; Gyou="+Gyou);
			if(0<Gyou){
				if(NR!="c" && WR==""){
					if(UR(1)=="EHON") bro.setInterval("STR_TXT();",1000,1);
					else STR_TXT();
				}
				else{
//CD("debug4","");
					for(i=1; i<=Gyou; i++){
						Pid2=Pid;
						if(WR=="w" && W_BTB[(PG-1)][(i-1)]==1) Pid2=Pid+"w";
						else if(WR=="r" && R_BTB[(PG-1)][(i-1)]==1) Pid2=Pid+"r";
//DEBUG4(Pid2);
//						WD(Pid+"_"+i,334);
						WD(Pid2+"_"+i,334);
					}
				}
			}
			LKS(0);
			if(AutPlay){
				PSI=bro.setInterval("PG_CHG();",Interval,1);
			}
			CHG=0;
			if(NR!="c"){
				if( GET("bgm").data=="ehon.aac"){
					STT(1);
					LoopSTR();
				}
				else STT(1);
			}
//			else if(UR(1)=="EHON") STT(1);
		}
	}
	else{
		LKS(1);
		N=0;
		if(BTB!=null){
//CD("debug4","");
			for(i=0; i<PMX; i++){
				P=BTB[i][0];
				if(N==P) continue;
				Pid="p"+(P-1);
				WD(Pid,0);
				LF(Pid+"d",0);
				for(j=1; j<=BTB[i][4]; j++){
					Pid2=Pid;
					if(WR=="w" && W_BTB[(P-1)][(j-1)]==1) Pid2=Pid+"w";
					else if(WR=="r" && R_BTB[(P-1)][(j-1)]==1) Pid2=Pid+"r";
//DEBUG4("DEL:"+Pid2);
//					WD(Pid+"_"+j,0);
					WD(Pid2+"_"+j,0);
				}
			}
		}
		LKS(0);
		if(NR!="c"){
			STT(0);
			DTC("bgm","ehon.aac");
			STT(1);
		}
		WD("p0",812);
	}
}
function PG_CHG(){
	if(Key){
		keyUp();
	}
	RECORD++;
	if(!isNaN(PSI) && 0<=PSI) bro.clearTimer(PSI);
	if(BTB!=null && PMX<=RECORD){
//DEBUG4("ここ"+PMX+"; "+RECORD);
		return;
	}
	if(BTB!=null){
		if(0<=RECORD) NX=BTB[RECORD][0];
	}
	CHG=1;
	PGSET();
}
function FD_OUT(){
	PosCnt++;
	var i,j;
	var Pid="p"+(PG-1);
	if(PosCnt==10){
		if(!isNaN(FSI) && 0<=FSI) bro.clearTimer(FSI);
		PosCnt=0;
		LKS(1);
		if(0<PG){
			WD(Pid,0);
			LF(Pid+"d",0);
		}
		if(BTB!=null){
			if(0<PG){
				for(j=1; j<=Gyou; j++){
					WD(Pid+"_"+j,0);
				}
			}
			PG=BTB[RECORD][0];
			TYPE=BTB[RECORD][1];
			Interval=BTB[RECORD][3]*1000;
			if(NR=="c") Interval=30000;
			Gyou=BTB[RECORD][4];
		}
		if(0<Gyou){
			if(NR!="c"){
				if(UR(1)=="EHON") bro.setInterval("STR_TXT();",1000,1);
				else STR_TXT();
			}
			else{
				for(i=1; i<=Gyou; i++) WD(Pid+"_"+i,334);
			}
		}
		LKS(0);
		if(AutPlay){
			PSI=bro.setInterval("PG_CHG();",Interval,1);
		}
		CHG=0;
		if(NR!="c"){
			if( GET("bgm").data=="ehon.aac"){
				LoopSTR();
			}
			else STT(1);
		}
		else if(UR(1)=="EHON") STT(1);
	}
	else{
		var lpos=-81*PosCnt;
		LF(Pid+"d",lpos);
	}
}
var nowGyou=0;
var TxCnt=0;
var TxEnd=0;
function STR_TXT(){
	if(Gyou==0) return;
	nowGyou=1;
	TxCnt=0;
	var Tint=(Interval-2500)/Gyou;
	TxEnd=Tint/200;
	if(!isNaN(TSI) && 0<=TSI) bro.clearTimer(TSI);
	TSI=bro.setInterval("VIS_TXT();",200,0);
}
function VIS_TXT(){
	TxCnt++;
	var Pid="p"+(PG-1)+"_"+nowGyou;
//	if(TxCnt==5) WD(Pid,334);
	if(TxCnt==TxEnd){
		WD(Pid,334);
		if(Gyou==nowGyou){
			if(!isNaN(TSI) && 0<=TSI) bro.clearTimer(TSI);
			return;
		}
		nowGyou++;
		TxCnt=0;
	}
	else{
//		var W=334/TxEnd;
//		W=W*TxCnt;
//		WD(Pid,W);
	}
}
function BL(){
	if(FID=="") return;
	VH(FID+"n",1);
	VH(FID+"d",0);
}
function DW(){
	if(FID=="") return;
	VH(FID+"d",1);
	VH(FID+"n",0);
}
function keyDown(){
	if(CHG || Key) return;
	KNo=document.currentEvent.keyCode;
	if(!(KNo==19 || (21<=KNo && KNo<=24) )) return;
	if( (21<=KNo && KNo<=24) || KNo==19){
//		if( (RECORD<0 && KNo!=23) || (0<=RECORD && NR!="c") ){
//			STT(0);
//			LoopSTP();
//			bro.playRomSound("romsound://7");
//		}
		if(0<=SSI){
			browser.clearTimer(SSI);
			SSI=-1;
		}
		LoopSTP();
		STT(0);
		PR(7);
	}
	if(KNo==21){
		if(Myself) FID="bl3";
		else if(0<=RECORD) FID="bl2";
		else FID="bl1";
		DW();
	}
	if(KNo==22){
		if(Myself) FID="rd3";
		else if(0<=RECORD) FID="rd2";
		else FID="rd1";
		DW();
	}
	if(KNo==23){
		if(Myself) FID="gr3";
		else if(0<=RECORD) FID="gr2";
		else FID="gr1";
		DW();
	}
	if(KNo==24){
		if(Myself) FID="yl3";
		else if(0<=RECORD) FID="yl2";
		else FID="yl1";
		DW();
	}
	if(KNo==19){
		BL();
	}
	if(KNo!=23){
		if(!isNaN(PSI) && 0<=PSI) bro.clearTimer(PSI);
		if(!isNaN(TSI) && 0<=TSI) bro.clearTimer(TSI);
	}
	Key=1;
}
function keyUp(){
	if(CHG || Key==0) return;
	if(!(KNo==19 || (21<=KNo && KNo<=24) )) return;
	var i;
	if(KNo==19){
		if(0<=RECORD || Myself){
			RELOAD_PG();
		}
		else bro.launchDocument("../startup.bml","cut");
	}
	var timing=0;
	if(KNo==21){
		if(Myself){
			Myself=0;
			RECORD=0;
			NR="f";
			WR="w";
			AutPlay=1;
			timing=1;
		}
		else if(0<=RECORD){
			RECORD--;
			if(21<=RECORD) RECORD=20;
		}
		else{
			RECORD=0;
			NR="f";
			WR="";
			AutPlay=1;
			timing=1;
		}
	}
	if(KNo==22){
		if(Myself){
			Myself=0;
			RECORD=0;
			NR="w";
			WR="r";
			AutPlay=1;
			timing=1;
		}
		else if(0<=RECORD){
			RECORD++;
			if(BTB.length<=RECORD) RECORD=-1;
		}
		else{
			RECORD=0;
			NR="w";
			WR="";
			AutPlay=1;
			timing=1;
		}
	}
	if(KNo==23){
		if(Myself){
			Myself=0;
			RECORD=0;
			AutPlay=1;
			timing=1;
		}
		else if(0<=RECORD){
			// 目次へ
			if(!isNaN(PSI) && 0<=PSI) bro.clearTimer(PSI);
			if(!isNaN(TSI) && 0<=TSI) bro.clearTimer(TSI);
			UW(3,NR);
			UW(4,WR);
			bro.launchDocument("mokuji.bml","cut");
		}
		else{
			Myself=1;
			NR="c";
			WR="";
		}
		BL();
	}
	if(KNo==24){
		if(0<=RECORD || Myself){
			// 最初へ
			RELOAD_PG();
		}
		else{
			// 目次へ
//			UW(3,NR);
//			UW(4,WR);
//			bro.launchDocument("mokuji.bml","cut");
			bro.launchDocument("../startup.bml","cut");
		}
		BL();
	}
	if(BTB!=null){
		if(0<=RECORD) NX=BTB[RECORD][0];
	}
	PGSET();
	BL();
	Key=0;
}
function RELOAD_PG(){
	// 最初へ
//DEBUG(RECORD+"; "+Gyou);
	var i;
	for(i=1; i<=Gyou; i++){
		WD("p"+RECORD+"_"+i,0);
	}
	RECORD=-1;
	AutPlay=0;
	Gyou=0;
	PG=1;
	Myself=0;
}
var doc=document;
var bro=browser;
function GET(id){ return doc.getElementById(id); }
function STY(id){ return GET(id).normalStyle; }
function CD(id,dat){ GET(id).firstChild.data=dat.toString(); }
function VH(id,v){ STY(id).visibility=(v==1)?"visible":"hidden"; }
function LF(id,l){ STY(id).left=l+"px"; }
function WD(id,w){ STY(id).width=w+"px"; }
function UR(n){ return bro.Ureg[n]; }
function UW(n,dat){ bro.Ureg[n]=dat.toString(); }
function PR(n){
//DEBUG2("romsound");
	bro.playRomSound("romsound://"+n);
}
var SSI=-1;
function STT(p){
	var dat=(p==1)?"play":"stop";
	if(p==1) SSI=browser.setInterval("STP();",1000,1);
	else{
		GET("bgm").streamStatus=(p==1)?"play":"stop";
//DEBUG2(GET("bgm").data+"; "+GET("bgm").streamStatus);
	}
}
function STP(){
	GET("bgm").streamStatus="play";
//DEBUG2(GET("bgm").data+"; "+GET("bgm").streamStatus);
}
function DTC(id,dat){ GET(id).data=dat.toString(); }
function NUM(dat){ return parseInt(dat,10); }
function LKS(l){
	if(l) bro.lockScreen();
	else bro.unlockScreen();
}
function GBT(file,str) {
	var bt=new Array();
	var t_b=new BinaryTable(file,str);
	if(t_b==null) return null;
	bt=t_b.toArray(0,t_b.nrow);
	t_b.close();
	return bt;
}
function DEBUG(dat){ CD("debug",dat); }
function DEBUG2(dat){ CD("debug2",dat); }
function DEBUG3(dat){ CD("debug3",dat); }
function DEBUG4(dat){
	CD("debug4",GET("debug4").firstChild.data+dat+";\r\n");
}
]]></script>
