<?xml version="1.0" encoding="EUC-JP" ?><!DOCTYPE bml PUBLIC "-//ARIB STD-B24:1999//DTD BML Document for IPTV//JA" "http://www.arib.or.jp/B24/DTD/bml_x_x_iptv.dtd"><?bml bml-version="100.0" ?><bml>	<head>		<title/><script><![CDATA[var d=document;var b=browser;var Fid="tag1";var FM=new Array();	// U,D,L,R	FM["tag1"]=new Array("","","tag8","tag2");	FM["tag2"]=new Array("","","tag1","tag3");	FM["tag3"]=new Array("","","tag2","tag8");	FM["tag7"]=new Array("","","tag3","tag8");	FM["tag8"]=new Array("","","tag3","tag1");	FM["tag4"]=new Array("","","","");	FM["tag5"]=new Array("","","","");	FM["tag6"]=new Array("","","","");var KD=0;var K;var PG=0;var NX=0;function load(){	GE("bgm").streamStatus="play";//	VH("bg1",1);//	VH("bg2",1);	LK(1);	FC(Fid);	LK(0);	b.setInterval("BGMrp();",31000,0);}function FC(id){	VH(id,0);	VH(id+"d",0);	VH(id+"f",1);	if(Fid=="tag1" || Fid=="tag2" || Fid=="tag3"){		if(Fid=="tag1") NX=0;		if(Fid=="tag2") NX=1;		if(Fid=="tag3") NX=2;		PGset();	}}function FB(id){	VH(id+"d",0);	VH(id+"f",0);	VH(id,1);}function DW(id){	VH(id,0);	VH(id+"f",0);	VH(id+"d",1);}function keyDown(){	if(KD==1) return;	K=d.currentEvent.keyCode;	// UDLR	if(1<=K && K<=4){		if(FM[Fid][(K-1)]=="") return;		LK(1);		FB(Fid);		Fid=FM[Fid][(K-1)];		LK(0);	}	if(K==18){		if(Fid=="tag1" || Fid=="tag2" || Fid=="tag3") return;		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		DW(Fid);		LK(0);	}	if(K==19){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		FB(Fid);		LK(0);	}	KD=1;}function keyUp(){	if(KD==0) return;	// UDLR	if(1<=K && K<=4){		LK(1);		FC(Fid);		LK(0);	}	if(K==18){		if(Fid=="tag8") LC("fp_a1.bml");	}	if(K==19){		b.launchDocument("fp_a1.bml","cut");	}	KD=0;}function PGset(){	if(NX==0){		LK(1);		if(PG==1){					}		if(PG==2){					}		if(PG==3){					}		LK(0);		LK(1);		VH("background1",1);		VH("tag6",1);		if(PG==1) VH("background2",0);		if(PG==2) VH("background3",0);		LK(0);	}	else if(NX==1){		LK(1);		VH("tag6",0);		VH("background2",1);//		if(PG==0) VH("background1",0);		if(PG==2) VH("background3",0);		LK(0);	}	else if(NX==2){		LK(1);		VH("tag6",0);		VH("background3",1);		if(PG==1) VH("background2",0);//		if(PG==0) VH("background1",0);		LK(0);	}	PG=NX;}function GE(id){	return d.getElementById(id);}function VH(id,v){	GE(id).normalStyle.visibility=(v)?"visible":"hidden";}function LK(f){	if(f) b.lockScreen();	else b.unlockScreen();}function LC(bml){	GE("bgm").streamStatus="stop";	b.launchDocument(bml,"cut");}function BGMrp(){	GE("bgm").streamStatus="stop";	GE("bgm").streamStatus="play";}]]></script>	</head>	<body id="body1" onload="load();" style="resolution:960x540; display-aspect-ratio:16v9"><!--		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="bg1" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_7.jpg" type="image/jpeg"/>			<object id="bg2" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_8.jpg" type="image/jpeg"/>		</div>-->		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="background1" style="left:0px; top:0px; width:960px; height:540px" data="background_fpa_6.jpg" type="image/jpeg"/><!--			<object id="background2" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_7.jpg" type="image/jpeg"/>			<object id="background3" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_8.jpg" type="image/jpeg"/>-->			<div id="background2" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;">				<object style="left:30px; top:24px; width:291px; height:372px;" data="background_fpa_7_1.jpg" type="image/jpeg"/>				<object style="left:349px; top:24px; width: