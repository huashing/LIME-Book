<?xml version="1.0" encoding="EUC-JP" ?><!DOCTYPE bml PUBLIC "-//ARIB STD-B24:1999//DTD BML Document for IPTV//JA" "http://www.arib.or.jp/B24/DTD/bml_x_x_iptv.dtd"><?bml bml-version="100.0" ?><bml>	<head>		<title/><script><![CDATA[var d=document;var b=browser;var Fid="tag1";var FM=new Array();	// U,D,L,R	FM["tag1"]=new Array("","","tag4","tag2");	FM["tag2"]=new Array("","","tag1","tag3");	FM["tag3"]=new Array("","","tag2","tag4");	FM["tag4"]=new Array("","","tag3","tag1");	FM["tag5"]=new Array("","","","");	FM["tag6"]=new Array("","","","");	FM["tag7"]=new Array("","","","");var KD=0;var K;var PG=0;var NX=0;function load(){	GE("bgm").streamStatus="play";/*	VH("bg1",1);	VH("bg2",1);	VH("bg3",1);*/	LK(1);	FC(Fid);	LK(0);	b.setInterval("BGMrp();",31000,0);}function FC(id){	VH(id,0);	VH(id+"d",0);	VH(id+"f",1);}function FB(id){	VH(id+"d",0);	VH(id+"f",0);	VH(id,1);}function DW(id){	VH(id,0);	VH(id+"f",0);	VH(id+"d",1);}function keyDown(){	if(KD==1) return;	K=d.currentEvent.keyCode;	// UDLR	if(1<=K && K<=4){		if(FM[Fid][(K-1)]=="") return;		LK(1);		FB(Fid);		Fid=FM[Fid][(K-1)];		LK(0);	}	if(K==18){		if(Fid=="tag4"){			GE("bgm").streamStatus="stop";			b.playRomSound("romsound://7");		}		if(Fid=="tag1") NX=1;		if(Fid=="tag2") NX=2;		if(Fid=="tag3") NX=3;		if(Fid=="tag5") NX=0;		if(Fid=="tag6") NX=0;		if(Fid=="tag7") NX=0;		LK(1);		DW(Fid);		LK(0);	}	if(K==19){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		FB(Fid);		LK(0);	}	KD=1;}function keyUp(){	if(KD==0) return;	// UDLR	if(1<=K && K<=4){		LK(1);		FC(Fid);		LK(0);	}	if(K==18){		if(Fid=="tag4") LC("fp_a1.bml");		else{			PGset();		}	}	if(K==19){		b.launchDocument("fp_a1.bml","cut");	}	KD=0;}function PGset(){	if(NX==0){		LK(1);		if(PG==1){			VH("tag5",0);			VH("tag5f",0);			VH("tag5d",0);			VH("tag3",1);			VH("tag3d",0);			VH("tag4",1);			VH("tag4d",0);		}		if(PG==2){			VH("tag6",0);			VH("tag6f",0);			VH("tag6d",0);		}		if(PG==3){			VH("tag7",0);			VH("tag7f",0);			VH("tag7d",0);		}		LK(0);		LK(1);		if(Fid=="tag5") Fid="tag1";		else if(Fid=="tag6") Fid="tag2";		else if(Fid=="tag7") Fid="tag3";		FC(Fid);		LK(0);		LK(1);		VH("background1",1);		if(PG==1) VH("background2",0);		if(PG==2) VH("background3",0);		if(PG==3) VH("background4",0);		LK(0);		LK(1);		if(PG==1){			VH("tag2",1);			VH("tag3",1);			VH("tag3d",0);			VH("tag4",1);			VH("tag4d",0);		}		if(PG==2){			VH("tag3",1);			VH("tag4",4);			VH("tag1",1);			VH("tag1d",0);		}		if(PG==3){			VH("tag2",1);			VH("tag1",1);			VH("tag1d",0);			VH("tag4",1);			VH("tag4d",0);		}		LK(0);	}	else{		LK(1);		if(NX==1){			VH("tag2",0);			VH("tag2f",0);			VH("tag2d",0);			VH("tag3d",1);			VH("tag3",0);			VH("tag4d",1);			VH("tag4",0);		}		if(NX==2){			VH("tag3",0);			VH("tag3f",0);			VH("tag3d",0);			VH("tag4",0);			VH("tag4f",0);			VH("tag4d",0);			VH("tag1d",1);			VH("tag1",0);		}		if(NX==3){			VH("tag2",0);			VH("tag2f",0);			VH("tag2d",0);			VH("tag1d",1);			VH("tag1",0);			VH("tag4d",1);			VH("tag4",0);		}		LK(0);		LK(1);		if(NX==1) VH("background2",1);		if(NX==2) VH("background3",1);		if(NX==3) VH("background4",1);//		VH("background1",0);		LK(0);		LK(1);//		FB(Fid);		if(NX==1){			VH("tag5",1);			Fid="tag5";		}		if(NX==2){			VH("tag6",1);			Fid="tag6";		}		if(NX==3){			VH("tag7",1);			Fid="tag7";		}		FC(Fid);		LK(0);	}	PG=NX;}function GE(id){	return d.getElementById(id);}function VH(id,v){	GE(id).normalStyle.visibility=(v)?"visible":"hidden";}function LK(f){	if(f) b.lockScreen();	else b.unlockScreen();}function LC(bml){	GE("bgm").streamStatus="stop";	b.launchDocument(bml,"cut");}function BGMrp(){	GE("bgm").streamStatus="stop";	GE("bgm").streamStatus="play";}]]></script>	</head>	<body id="body1" onload="load();" style="resolution:960x540; display-aspect-ratio:16v9"><!--		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="bg1" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_3.jpg" type="image/jpeg"/>			<object id="bg2" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_4.jpg" type="image/jpeg"/>			<object id="bg3" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_5.jpg" type="image/jpeg"/>		</div>-->		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="background1" style="left:0px; top:0px; width:960px; height:540px" data="background_fpa_2.jpg" type="image/jpeg"/><!--			<object id="background2" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_3.jpg" type="image/jpeg"/>			<object id="background3" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_4.jpg" type="image/jpeg"/>			<object id="background4" style="left:0px; top:0px; width:960px; height:540px; visibility:hidden;" data="background_fpa_5.jpg" type="image/jpeg"/>-->			<object id="background2" style="left:320px; top:20px; width:301px; height:501px; visibility:hidden;" data="background_fpa_3.jpg" type="image/jpeg"/>			<object id="background3" style="left:640px; top:20px; width:301px; height:501px; visibility:hidden;" data="background_fpa_4.jpg" type="image/jpeg"/>			<object id="background4" style="left:339px; top:30px; width:301px; height:492px; visibility:hidden;" data="background_fpa_5.jpg" type="image/jpeg"/>		</div>		<div>			<object id="bgm" data="1s.aac" type="audio/X-arib-mpeg2-aac" streamstatus="stop"/>		</div>		<div id="btn" style="nav-index:0" onkeydown="keyDown();" onkeyup="keyUp();"/>		<div style="left:15px; top:498px; width:141px; height:33px; background-color-index:8">			<object id="tag1" style="left:0px; top:0px; width:141px; height:33px" data="normal_01_fpa_2.jpg" type="image/jpeg"/>			<object id="tag1f" style="left:0px; top:0px; width:141px; height:33px; visibility:hidden;" data="focus_01_fpa_2.jpg" type="image/jpeg"/>			<object id="tag1d" style="left:0px; top:0px; width:141px; height:33px; visibility:hidden;" data="keydown_01_fpa_2.jpg" type="image/jpeg"/>		</div>		<div style="left:333px; top:498px; width:144px; height:33px; background-color-index:8">			<object id="tag2" style="left:0px; top:0px; width:144px; height:33px" data="normal_02_fpa_2.jpg" type="image/jpeg"/>			<object id="tag2f" style="left:0px; top:0px; width:144px; height:33px; visibility:hidden;" data="focus_02_fpa_2.jpg" type="image/jpeg"/>			<object id="tag2d" style="left:0px; top:0px; width:144px; height:33px; visibility:hidden;" data="keydown_02_fpa_2.jpg" type="image/jpeg"/>		</div>		<div style="left:654px; top:498px; width:141px; height:33px; background-color-index:8">			<object id="tag3" style="left:0px; top:0px; width:141px; height:33px" data="normal_03_fpa_2.jpg" type="image/jpeg"/>			<object id="tag3f" style="left:0px; top:0px; width:141px; height:33px; visibility:hidden;" data="focus_03_fpa_2.jpg" type="image/jpeg"/>			<object id="tag3d" style="left:0px; top:0px; width:141px; height:33px; visibility:hidden;" data="keydown_03_fpa_2.jpg" type="image/jpeg"/>		</div>		<div style="left:852px; top:507px; width:96px; height:24px; background-color-index:8">			<object id="tag4" style="left:0px; top:0px; width:96px; height:24px" data="normal_04_fpa_2.jpg" type="image/jpeg"/>			<object id="tag4f" style="left:0px; top:0px; width:96px; height:24px; visibility:hidden;" data="focus_04_fpa_2.jpg" type="image/jpeg"/>			<object id="tag4d" style="left:0px; top:0px; width:96px; height:24px; visibility:hidden;" data="keydown_04_fpa_2.jpg" type="image/jpeg"/>		</div>		<div style="left:558px; top:492px; width:51px; height:24px; background-color-index:8">			<object id="tag5" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="normal_03_fpa_3.jpg" type="image/jpeg"/>			<object id="tag5f" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="focus_03_fpa_3.jpg" type="image/jpeg"/>			<object id="tag5d" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="keydown_03_fpa_3.jpg" type="image/jpeg"/>		</div>		<div style="left:876px; top:492px; width:51px; height:24px; background-color-index:8">			<object id="tag6" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="normal_03_fpa_4.jpg" type="image/jpeg"/>			<object id="tag6f" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="focus_03_fpa_4.jpg" type="image/jpeg"/>			<object id="tag6d" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="keydown_03_fpa_4.jpg" type="image/jpeg"/>		</div>		<div style="left:576px; top:492px; width:54px; height:24px; background-color-index:8">			<object id="tag7" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="normal_03_fpa_5.jpg" type="image/jpeg"/>			<object id="tag7f" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="focus_03_fpa_5.jpg" type="image/jpeg"/>			<object id="tag7d" style="left:0px; top:0px; width:51px; height:24px; visibility:hidden;" data="keydown_03_fpa_5.jpg" type="image/jpeg"/>		</div>	</body></bml>