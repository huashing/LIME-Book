<?xml version="1.0" encoding="EUC-JP" ?><!DOCTYPE bml PUBLIC "-//ARIB STD-B24:1999//DTD BML Document for IPTV//JA" "http://www.arib.or.jp/B24/DTD/bml_x_x_iptv.dtd"><?bml bml-version="100.0" ?><bml>	<head>		<title/><script><![CDATA[var d=document;var b=browser;var Fid="tag1";var FM=new Array();	// U,D,L,R	FM["tag1"]=new Array("","","tag3","tag2");	FM["tag2"]=new Array("","","tag1","tag3");	FM["tag3"]=new Array("","","tag2","tag1");var KD=0;var K;function load(){	GE("bgm").streamStatus="play";	LK(1);	FC(Fid);	LK(0);	b.setInterval("BGMrp();",31000,0);}function FC(id){	VH(id,0);	VH(id+"d",0);	VH(id+"f",1);}function FB(id){	VH(id+"d",0);	VH(id+"f",0);	VH(id,1);}function DW(id){	VH(id,0);	VH(id+"f",0);	VH(id+"d",1);}function keyDown(){	if(KD==1) return;	K=d.currentEvent.keyCode;	// UDLR	if(1<=K && K<=4){		if(FM[Fid][(K-1)]=="") return;		LK(1);		FB(Fid);		Fid=FM[Fid][(K-1)];		LK(0);	}	if(K==18){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		DW(Fid);		LK(0);	}	if(K==19){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		FB(Fid);		LK(0);	}	KD=1;}function keyUp(){	if(KD==0) return;	// UDLR	if(1<=K && K<=4){		LK(1);		FC(Fid);		LK(0);	}	if(K==18){		if(Fid=="tag1") LC("fp_a2.bml");		else if(Fid=="tag2") LC("fp_a3.bml");		else if(Fid=="tag3") LC("mainmenu.bml");	}	if(K==19){		b.launchDocument("mainmenu.bml","cut");	}	KD=0;}function GE(id){	return d.getElementById(id);}function VH(id,v){	GE(id).normalStyle.visibility=(v)?"visible":"hidden";}function LK(f){	if(f) b.lockScreen();	else b.unlockScreen();}function LC(bml){	GE("bgm").streamStatus="stop";	b.launchDocument(bml,"cut");}function BGMrp(){	GE("bgm").streamStatus="stop";	GE("bgm").streamStatus="play";}]]></script>	</head>	<body id="body1" onload="load();" style="resolution:960x540; display-aspect-ratio:16v9">		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="background" style="left:0px; top:0px; width:960px; height:540px" data="background_fpa_1.jpg" type="image/jpeg"/>		</div>		<div>			<object id="bgm" data="1s.aac" type="audio/X-arib-mpeg2-aac" streamstatus="stop"/>		</div>		<div id="btn" style="nav-index:0" onkeydown="keyDown();" onkeyup="keyUp();"/>		<div style="left:21px; top:231px; width:189px; height:189px; background-color-index:8">			<object id="tag1" style="left:6px; top:24px; width:159px; height:156px" data="normal_01_fpa_1.jpg" type="image/jpeg"/>			<object id="tag1f" style="left:0px; top:0px; width:189px; height:189px; visibility:hidden;" data="focus_01_fpa_1.jpg" type="image/jpeg"/>			<object id="tag1d" style="left:0px; top:0px; width:189px; height:189px; visibility:hidden;" data="keydown_01_fpa_1.jpg" type="image/jpeg"/>		</div>		<div style="left:501px; top:231px; width:189px; height:189px; background-color-index:8">			<object id="tag2" style="left:9px; top:24px; width:156px; height:156px" data="normal_02_fpa_1.jpg" type="image/jpeg"/>			<object id="tag2f" style="left:0px; top:0px; width:189px; height:189px; visibility:hidden;" data="focus_03_fpa_1.jpg" type="image/jpeg"/>			<object id="tag2d" style="left:0px; top:0px; width:189px; height:189px; visibility:hidden;" data="keydown_02_fpa_1.jpg" type="image/jpeg"/>		</div>		<div style="left:852px; top:507px; width:96px; height:24px; background-color-index:8">			<object id="tag3" style="left:0px; top:0px; width:96px; height:24px" data="normal_05_fpa_1.jpg" type="image/jpeg"/>			<object id="tag3f" style="left:0px; top:0px; width:96px; height:24px; visibility:hidden;" data="focus_05_fpa_1.jpg" type="image/jpeg"/>			<object id="tag3d" style="left:0px; top:0px; width:96px; height:24px; visibility:hidden;" data="keydown_05_fpa_1.jpg" type="image/jpeg"/>		</div>	</body></bml>