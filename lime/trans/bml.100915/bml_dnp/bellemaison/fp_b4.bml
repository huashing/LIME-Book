<?xml version="1.0" encoding="EUC-JP" ?><!DOCTYPE bml PUBLIC "-//ARIB STD-B24:1999//DTD BML Document for IPTV//JA" "http://www.arib.or.jp/B24/DTD/bml_x_x_iptv.dtd"><?bml bml-version="100.0" ?><bml>	<head>		<title/><script><![CDATA[var d=document;var b=browser;var Fid="tag4";var FM=new Array();	// U,D,L,R	FM["tag1"]=new Array("","tag4","","tag2");	FM["tag2"]=new Array("","tag4","tag1","tag3");	FM["tag3"]=new Array("","tag4","tag2","");	FM["tag4"]=new Array("tag2","tag5","","");	FM["tag5"]=new Array("tag4","","","");var KD=0;var K;function load(){	GE("bgm").streamStatus="play";	LK(1);	FC(Fid);	LK(0);	b.setInterval("BGMrp();",31000,0);}function FC(id){	VH(id,0);	VH(id+"d",0);	VH(id+"f",1);}function FB(id){	VH(id+"d",0);	VH(id+"f",0);	VH(id,1);}function DW(id){	VH(id,0);	VH(id+"f",0);	VH(id+"d",1);}function keyDown(){	if(KD==1) return;	K=d.currentEvent.keyCode;	// UDLR	if(1<=K && K<=4){		if(FM[Fid][(K-1)]=="") return;//		b.playRomSound("romsound://9");		LK(1);		FB(Fid);		Fid=FM[Fid][(K-1)];		LK(0);	}	if(K==18){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		DW(Fid);		LK(0);	}	if(K==19){		GE("bgm").streamStatus="stop";		b.playRomSound("romsound://7");		LK(1);		FB(Fid);		LK(0);	}	KD=1;}function keyUp(){	if(KD==0) return;	// UDLR	if(1<=K && K<=4){		LK(1);		FC(Fid);		LK(0);	}	if(K==18){		if(b.Ureg[1]=="") b.Ureg[1]="fp_b4.bml";		if(Fid=="tag1") LC("fp_a1.bml");		else if(Fid=="tag2") LC("fp_b1.bml");		else if(Fid=="tag3") LC("homebase1.bml");		else if(Fid=="tag4") LC(b.Ureg[1]);		else if(Fid=="tag5") LC(b.Ureg[1]);	}	if(K==19){		if(b.Ureg[1]=="") b.Ureg[1]="fp_b4.bml";		b.launchDocument(b.Ureg[1],"cut");	}	KD=0;}function GE(id){	return d.getElementById(id);}function VH(id,v){	GE(id).normalStyle.visibility=(v)?"visible":"hidden";}function LK(f){	if(f) b.lockScreen();	else b.unlockScreen();}function LC(bml){	GE("bgm").streamStatus="stop";	b.launchDocument(bml,"cut");}function BGMrp(){	GE("bgm").streamStatus="stop";	GE("bgm").streamStatus="play";}]]></script>	</head>	<body id="body1" onload="load();" style="resolution:960x540; display-aspect-ratio:16v9">		<div style="left:0px; top:0px; width:960px; height:540px; background-color-index:8">			<object id="background" style="left:0px; top:0px; width:960px; height:540px" data="background_fpb_4.jpg" type="image/jpeg"/>		</div>		<div>			<object id="bgm" data="6s.aac" type="audio/X-arib-mpeg2-aac" streamstatus="stop"/>		</div>		<div id="btn" style="nav-index:0" onkeydown="keyDown();" onkeyup="keyUp();"/>		<div style="left:9px; top:12px; width:204px; height:51px; background-color-index:8">			<object id="tag1" style="left:0px; top:0px; width:204px; height:51px" data="normal_01_fpb_4.jpg" type="image/jpeg"/>			<object id="tag1f" style="left:0px; top:0px; width:204px; height:48px; visibility:hidden;" data="focus_01_fpb_4.jpg" type="image/jpeg"/>			<object id="tag1d" style="left:0px; top:0px; width:204px; height:48px; visibility:hidden;" data="keydown_01_fpb_4.jpg" type="image/jpeg"/>		</div>		<div style="left:219px; top:6px; width:354px; height:54px; background-color-index:8">			<object id="tag2" style="left:0px; top:0px; width:354px; height:54px" data="normal_02_fpb_4.jpg" type="image/jpeg"/>			<object id="tag2f" style="left:0px; top:0px; width:354px; height:57px; visibility:hidden;" data="focus_02_fpb_4.jpg" type="image/jpeg"/>			<object id="tag2d" style="left:0px; top:0px; width:354px; height:57px; visibility:hidden;" data="keydown_02_fpb_4.jpg" type="image/jpeg"/>		</div>		<div style="left:579px; top:12px; width:204px; height:48px; background-color-index:8">			<object id="tag3" style="left:0px; top:0px; width:204px; height:48px" data="normal_03_fpb_4.jpg" type="image/jpeg"/>			<object id="tag3f" style="left:0px; top:0px; width:204px; height:48px; visibility:hidden;" data="focus_03_fpb_4.jpg" type="image/jpeg"/>			<object id="tag3d" style="left:0px; top:0px; width:204px; height:48px; visibility:hidden;" data="keydown_03_fpb_4.jpg" type="image/jpeg"/>		</div><!--		<div style="left:708px; top:249px; width:222px; height:60px; background-color-index:8">			<object id="tag4" style="left:0px; top:0px; width:222px; height:60px" data="normal_04_fpb_4.jpg" type="image/jpeg"/>			<object id="tag4f" style="left:0px; top:0px; width:222px; height:60px; visibility:hidden;" data="focus_04_fpb_4.jpg" type="image/jpeg"/>			<object id="tag4d" style="left:0px; top:0px; width:222px; height:60px; visibility:hidden;" data="keydown_04_fpb_4.jpg" type="image/jpeg"/>		</div>-->		<div style="left:762px; top:252px; width:177px; height:42px; background-color-index:8">			<object id="tag4" style="left:0px; top:0px; width:177px; height:42px" data="normal_04_fpb_4.jpg" type="image/jpeg"/>			<object id="tag4f" style="left:0px; top:0px; width:177px; height:42px; visibility:hidden;" data="focus_04_fpb_4.jpg" type="image/jpeg"/>			<object id="tag4d" style="left:0px; top:0px; width:177px; height:42px; visibility:hidden;" data="keydown_04_fpb_4.jpg" type="image/jpeg"/>		</div>		<div style="left:798px; top:306px; width:141px; height:33px; background-color-index:8">			<object id="tag5" style="left:0px; top:0px; width:141px; height:33px" data="normal_05_fpb_4.jpg" type="image/jpeg"/>			<object id="tag5f" style="left:0px; top:0px; width:141px; height:33px; visibility:hidden;" data="focus_05_fpb_4.jpg" type="image/jpeg"/>			<object id="tag5d" style="left:0px; top:0px; width:141px; height:33px; visibility:hidden;" data="keydown_05_fpb_4.jpg" type="image/jpeg"/>		</div>	</body></bml>