//パレードキー押下処理
function prkey() {
	var code=document.currentEvent.keyCode;
	var id=document.currentEvent.target.id;
	var idx;
	//上キー
	if(code==1){
		idx=gParadeList.displaynum;
		//一番上で
		if(idx==0){
			if(gParadeList.totalCount>5){
				gParadeList.stopTimer();
				gParadeList.startnum=gParadeList.startnum-1;
				if(gParadeList.startnum<0)gParadeList.startnum=gParadeList.totalCount-1;
				playSound(9);
				if(gParadeList.playMovie)StopVideo();
				setText("text1","");
				setText("text2","");
				setText("copy","");
//				gParadeList.showList();
				if(gParadeList.showList()==-1){
					ClearParade();
					return;
				}
				gParadeList.showData();
			}
		}else{
			if(parade_focus(idx-1)!=-1){
				gParadeList.showData();
				setTrailerTimer();
			}
		}
		//トレイラーがあったら動画タイマを仕掛け直す
		if(!isNaN(gParadeList.movTimer))browser.clearTimer(gParadeList.movTimer);
		gParadeList.movTimer=NaN;
		var idx=gParadeList.nowIdx();
		if(String(gParadeList.contentList[idx].basicDescription.trailer)!="")gParadeList.movTimer=browser.setInterval("ExecParadeMov();",gParadeList.movTime,1);
	}else if(code==2){	//一番下で下キー
		idx=gParadeList.displaynum;
		if(idx==4){
			if(gParadeList.totalCount>5){
				gParadeList.stopTimer();
				gParadeList.startnum=gParadeList.startnum+1;
				if(gParadeList.startnum>=gParadeList.totalCount)gParadeList.startnum=0;
				playSound(9);
				if(gParadeList.playMovie)StopVideo();
				setText("text1","");
				setText("text2","");
				setText("copy","");
//				gParadeList.showList();
				if(gParadeList.showList()==-1){
					ClearParade();
					return;
				}
				gParadeList.showData();
			}
		}else{
			if(parade_focus(idx+1)!=-1){
				gParadeList.showData();
				setTrailerTimer();
			}
		}
		//トレイラーがあったら動画タイマを仕掛け直す
		if(!isNaN(gParadeList.movTimer))browser.clearTimer(gParadeList.movTimer);
		gParadeList.movTimer=NaN;
		var idx=gParadeList.nowIdx();
		if(String(gParadeList.contentList[idx].basicDescription.trailer)!="")gParadeList.movTimer=browser.setInterval("ExecParadeMov();",gParadeList.movTime,1);
	}else if(code==4){	//右キー
		if(!isNaN(gParadeList.movTimer))browser.clearTimer(gParadeList.movTimer);
		gParadeList.movTimer=NaN;
		if(!gParadeList.playMovie&&isNaN(gParadeList.scrolltimer)&&isNaN(gParadeList.firstTimer))gParadeList.scrolltimer=browser.setInterval("ExecParadeScrool();",gParadeList.scrollTime ,1);
		idx=gParadeList.displaynum;
		gParadeList.mode="menu";
		gParadeList.menuIdx=gParadeList.oldfocus;
		menu_focus(gParadeList.oldfocus);
		showElem("mask_"+gParadeList.displaynum);
		hideElem("pa_scrl");
	}else if(code==18){
		var idx=gParadeList.nowIdx();
		playSound(7);
		var target=gParadeList.contentList[idx];
		var type=GetDisplayTypeInfo(target.displayType);
		if(! type) return;
		ClearVideoTop();
		setImg("bg","images/bg_hanyo.png");
		gPlaySound=false;
		showByContentInformation(target.crid,target.displayType,true);
	}else if(code==19){
		returnFunc();
	}
}
//メニューキー押下処理
function menukey(){
	var code=document.currentEvent.keyCode;
	var id=gParadeList.menuIdx;
	var target="";
	if(code==1){
		if(id=="bt_nod_01")target="";
		else if(id=="bt_nod_02")target="bt_nod_01";
		else if(id=="bt_nod_03")target="bt_nod_02";
		else if(id=="bt_nod_04")target="bt_nod_03";
		else if(id=="bt_nod_05")target="bt_nod_04";
		else if(id=="bt_nod_06")target="bt_nod_05";
		else if(id=="bt_nod_07")target="bt_nod_06";
		if(target!="")menu_focus(target);
		else{
			hideElem("fcbtn");
			scFocus();
		}
	}else if(code==2){
		if(id=="bt_nod_01")target="bt_nod_02";
		else if(id=="bt_nod_02")target="bt_nod_03";
		else if(id=="bt_nod_03")target="bt_nod_04";
		else if(id=="bt_nod_04")target="bt_nod_05";
		else if(id=="bt_nod_05")target="bt_nod_06";
		else if(id=="bt_nod_06")target="bt_nod_07";
		else if(id=="bt_nod_06")target="";
		if(target!="")menu_focus(target);
		else{
			hideElem("fcbtn");
			scFocus();
		}
	}else if(code==3){	//左へ
		if(isNaN(gParadeList.displaynum))return;
		else {
			gParadeList.oldfocus=gParadeList.menuIdx;
			if(parade_focus(gParadeList.displaynum)==-1)return;
			if(gParadeList.playMovie==false) setTrailerTimer();
			hideElem("mask_"+gParadeList.displaynum);
			gParadeList.mode="parade";
			if(!isNaN(gParadeList.scrolltimer))browser.clearTimer(gParadeList.scrolltimer);
			gParadeList.scrolltimer=NaN;
		}
	}else if(code==18){
		playSound(7);
		target=0;
		if(id=="bt_nod_01")target=1;
		else if(id=="bt_nod_02")target=2;
		else if(id=="bt_nod_03")target=3;
		else if(id=="bt_nod_04")target=4;
		else if(id=="bt_nod_05")target=5;
		else if(id=="bt_nod_06")target=6;
		else if(id=="bt_nod_07")target=7;
		if(target>0){
			ClearVideoTop();
			showMenu(target);
		}
	}else if(code==19){
		hideElem("fcbtn");
		returnFunc();
	}
}
