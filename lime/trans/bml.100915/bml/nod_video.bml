function ID_click(code,ispid,mode,age){
	if(this.id.length==4){
		// 下部
		hideElem("idcoment2");
		gState.downbox = new ListA();
		gState.downbox.init(9,this.get(),searchIdList);
		gState.downbox.show(ispid,mode,age);
		if(gState.downbox.totalCount==0){
			setImg("base","images/bg_ptn03.png");
			showElem("idcoment3");
			getElementById("body").normalStyle.usedKeyList="basic data-button special-1 numeric-tuning"; // リストを消すと無効になるため
		}
		if(gState.downbox.isfocus()){
			id_blur();
			gPlaySound=false;
			gState.downbox.focus(-1);
		}
	}else if(this.id.length==5){
		hideElem("idcoment2");
		if(!gFOff){
			unlockScreen();
			keyOff();
			lockScreen();
		}
		var pi = seachId(this.id,gState.ispid,gState.mode,gState.age);
		if(pi!=null&&pi.totalCount==1){
			gPlaySound=false;
			showByContentInformation(pi.contentList[0].crid,pi.contentList[0].displayType,true);
		}else{
			showElem("idcoment3");
		}
	}
}
function idkey(){
	var code = document.currentEvent.keyCode;
	var id = document.currentEvent.target.id;
	lockScreen();
	if(code==1){
		if(!gUpDownKey) {unlockScreen();return;}
		id_blur();
		scFocus();
	}else if(code==2){
		if(!gUpDownKey) {unlockScreen();return;}
		if(gState.downbox!=null&&gState.downbox.isfocus()){
			id_blur();
			gState.downbox.focus(code);
		}else{
			id_blur();
			scFocus();
		}
	}else if(code==3){
		if(!gSideKey) {unlockScreen();return;}
		gState.upbox.backspace();
	}else if(code==4){
	}else if( (code >= 5) && (code <= 14) ){//数字キー
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		var num = (code-5);
		gState.upbox.add(num);
	}else if(21<=code && code<=24){
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		if(gState.downbox!=null){
			if(gState.downbox.list==null||gState.downbox.list.length==0) {unlockScreen();return;}
			if(gState.downbox.sort_keyflg[code-21]){id_blur();}
			gState.downbox.colorClick(code);
		}
	}else if(code==18){
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		playSound(7);
		gState.upbox.click(code,gState.ispid,gState.mode,gState.age);
	}else if(code==19){
		if(!gSideKey||!gUpDownKey) {unlockScreen();return;}
		gState.upbox.backspace();
	}
	unlockScreen();
}

