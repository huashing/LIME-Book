function getStopString(position){
	var time=Number(String(position).split(".")[0]);
	var h=0,m=0,s=0;
	s=time%60;
	time=time/60;
	m=time%60;
	time=time/60;
	h=time%60;
	return padding("  ",h)+"時間"+padding("  ",m)+"分"+padding("  ",s)+"秒";
}
