function PlayVideo(URL){
	if(!URL)return;
	if(!isNaN(this.fullScrTimer)){
		browser.clearTimer(this.fullScrTimer);
		this.fullScrTimer=NaN;
	}
	URL=String(URL);
	var ps=URL.indexOf("&drmid=$drmid$");
	if(ps>0)URL=URL.substring(0,ps)+URL.substring(ps+14);
	var drmId=getDRMID("marlin_iptv_es");
	if(drmId!=null&&String(drmId)!="NaN")URL+=("&drmid="+String(drmId.indexOf("0x")==0?drmId.substring(2):drmId).toLowerCase());
	var elm=getElementById("Video");
	elm.data=URL;
	showElem("guide_d01");
	showElem("Video");
	elm.streamStatus="play";
	hideElem("parade_img");
	setText("copy",gParadeList.contentList[gParadeList.nowIdx()].basicDescription.copyrightTrailer);
	gParadeList.playMovie=true;
	getElementById("MediaStopped").subscribe=true;
	getElementById("body").normalStyle.usedKeyList="basic data-button";
}
function StopVideo() {
	if(gParadeList.playMovie==false)return;
	gParadeList.playMovie=false;
	if(gParadeList.playFullscr==true)gParadeList.window();
	showElem("parade_img");
	hideElem("guide_d01");
	getElementById("body").normalStyle.usedKeyList="basic data-button special-1";
	getElementById("Video").streamStatus="stop";
	getElementById("MediaStopped").subscribe=false;
	hideElem("Video");
}

