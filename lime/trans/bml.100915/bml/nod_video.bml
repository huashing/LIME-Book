		git=getNodWizardGIT(genre,gState.ispid,gState.mode,gState.age);
		if(git!=null){
			if(git.totalCount==1) return git;
			else if(git.totalCount==0){
				showPopup(new Array("ホームへ"),gohome,"エラー","\r\nサーバーでエラーが発生しました。\r\n時間をおいて再度お試しください。\r\n\r\n状態番号：09087","");
				return null;
			}else return new PageInfomation(1,new Array(git.contentList[0]));
		}
		ret = getErrStat();
	}
	if(ret==null){
		showLoginErr();
	}else{
		showPopup(new Array("ホームへ"),gohome,"エラー",ret[1],ret[0]);
	}
	return null;

}
