				hideElem("loading2");
				showElem("finPop");
				unlockScreen();
				return false;
			}
			setErrScr("\r\n"+setErrMsg(getBmlErr("02","49")));
			return false;
		}
		return true;
	}else{gState.launchDocument("subscribe.bml");}
	return false;
}
