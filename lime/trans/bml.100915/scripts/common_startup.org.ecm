function showPopupbtn(btns){
	var btnLeft = new Array(new Array("222"),new Array("137","307"),new Array("52","222","392"));
	var btntLeft = new Array(new Array("230"),new Array("145","315"),new Array("60","230","400"));
	for(var i=0;i<3;i++){
