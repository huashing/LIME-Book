//パレードキー押下処理
function prkey(){
	var code=document.currentEvent.keyCode;
	var id=document.currentEvent.target.id;
	var idx;
	//上キー
	if(code==1){
		idx=gParadeList.displaynum;
		//一番上で
		if(idx==0){
			if(gParadeList.totalCount>5){
				gParadeList.stopTimer();
				gParadeList.startnum=gParadeList.startnum -1;
				if(gParadeList.startnum<0)gParadeList.startnum=gParadeList.totalCount-1;
				playSound(9);
				setText("text1","");
				setText("text2","");
//				gParadeList.showList();
				if(gParadeList.showList()==-1){
					ClearParade();
					return;
				}
				gParadeList.showData();
			}
		}else{parade_focus(idx-1);}
	}
	//一番下で下キー
	else if(code==2){
		idx=gParadeList.displaynum;
		if(idx==4){
			if(gParadeList.totalCount>5){
				gParadeList.stopTimer();
				gParadeList.startnum=gParadeList.startnum +1;
				if(gParadeList.startnum>=gParadeList.totalCount)gParadeList.startnum=0;
				playSound(9);
				setText("text1","");
				setText("text2","");
//				gParadeList.showList();
				if(gParadeList.showList()==-1){
					ClearParade();
					return;
				}
				gParadeList.showData();
			}
		}else{parade_focus(idx+1);}
	}
	//右キー
	else if(code==4){
		if(isNaN(gParadeList.scrolltimer)&&isNaN(gParadeList.firstTimer)){
			gParadeList.scrolltimer=browser.setInterval("ExecParadeScrool();", gParadeList.scrollTime ,1);
		}
		idx=gParadeList.displaynum;
		gParadeList.mode="menu";
		gParadeList.menuIdx=gParadeList.oldfocus;
		menu_focus(gParadeList.oldfocus);
		showElem("mask_"+gParadeList.displaynum);
		hideElem("pa_scrl");
	}
	else if(code==18){
		var idx=gParadeList.nowIdx();
		var type=GetDisplayTypeInfo(gParadeList.contentList[idx].displayType);
		if(! type) return;
		playSound(7);
		launchByContentInformation(gParadeList.contentList[idx].crid,gParadeList.contentList[idx].displayType);
	}
}
//メニューキー押下処理
function menukey(){
	var code=document.currentEvent.keyCode;
	var id=gParadeList.menuIdx;
	var target="";
	if(code==1){
		if(id=="bt_m_tv")target="bt_infod";
		else if(id=="bt_m_v")target="bt_m_tv";
		else if(id=="bt_m_ko")target="bt_m_v";
		else if(id=="bt_m_pr")target="bt_m_ko";
		else if(id=="bt_m_my")target="bt_m_pr";
		else if(id=="bt_m_nod")target="bt_m_my";
		else if(id=="bt_m_st")target="bt_m_nod";
		else if(id=="bt_m_info")target="bt_m_st";
		else if(id=="bt_infod")target="bt_m_info";
		if(target!="")menu_focus(target);
	}else if(code==2){
		if(id=="bt_m_tv")target="bt_m_v";
		else if(id=="bt_m_v")target="bt_m_ko";
		else if(id=="bt_m_ko")target="bt_m_pr";
		else if(id=="bt_m_pr")target="bt_m_my";
		else if(id=="bt_m_my")target="bt_m_nod";
		else if(id=="bt_m_nod")target="bt_m_st";
		else if(id=="bt_m_st")target="bt_m_info";
		else if(id=="bt_m_info")target="bt_infod";
		else if(id=="bt_infod")target="bt_m_tv";
		if(target!="")menu_focus(target);
	}else if(code==3){
		if(isNaN(gParadeList.displaynum))return;
		else if(id=="bt_infod")return;
		else {
			gParadeList.oldfocus=gParadeList.menuIdx;
			if(parade_focus(gParadeList.displaynum)==-1)return;
			hideElem("mask_"+gParadeList.displaynum);
			gParadeList.mode="parade";
		}
	}else if(code==4){
	}else if(code==18){
		if(id.indexOf("bt_m_tv")==0){// ＴＶへ
			playSound(7);
			lockScreen();
			gState.allHidden=null;
			gParadeList.stopTimer();
			gParadeList.jpgClear();
			setText("text1","");
			setText("text2","");
			hideElem("fcbtn");
			hideElem("pa_scrl");
			for(var i=0;i<5;i++){
				hideElem("pa_"+i);
				hideElem("mask_"+i);
				hideElem("icon_"+i);
			}
			unlockScreen();
			launchTv();
		}else if(id.indexOf("bt_m_v")==0){// ビデオへ
			playSound(7);
			launchVideo();
		}else if(id.indexOf("bt_m_ko")==0){// カラオケトップへ
			playSound(7);
			gState.launchDocument("karaoke_top.bml");
		}else if(id.indexOf("bt_m_pr")==0){// プレミアムへ
			playSound(7);
			gState.launchDocument("premium.bml");
		}else if(id.indexOf("bt_m_my")==0){// マイページへ
			playSound(7);
			launchMypage();
		}else if(id.indexOf("bt_m_nod")==0){// NODへ
			playSound(7);
			gState.launchDocument("nod_video.bml");
		}else if(id.indexOf("bt_m_st")==0){// 設定へ
			playSound(7);
			gState.launchDocument("config.bml");
		}else if(id.indexOf("bt_m_info")==0){// お知らせへ
			playSound(7);
			gState.launchDocument("news.bml");
		}else if(id.indexOf("bt_infod")==0){// お知らせへ
			playSound(7);
			if(gParadeList.announce!=null){
				gState.param_crid=String(gParadeList.announce.crid);
				gState.param_displayType=String(gParadeList.announce.displayType);
			}
			gState.launchDocument("news.bml");
		}
	}
}
function parade_focus(newIdx){
	if(newIdx>=gParadeList.totalCount)return -1;
	playSound(9);
	setImg("fcbtn","images/pa-f.png");
	var elm=getElementById("fcbtn");
	elm.normalStyle.left="60px";
	elm.normalStyle.top=(90+(69*newIdx))+"px";
	elm.normalStyle.width="164px";
	elm.normalStyle.height="78px";
	showElem("fcbtn");
	gParadeList.stopTimer();
	gParadeList.displaynum=newIdx;
	gParadeList.showData();
	hideElem("mask_"+gParadeList.displaynum);
	if(gParadeList.totalCount>5)showElem("pa_scrl");
	else hideElem("pa_scrl");
	return 0;
}
